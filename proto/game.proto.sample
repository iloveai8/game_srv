    required uint16 msgid = 1;
    required int32 index = 2; //要挑战的index
}

//个人挑战胜利可每天领取一次的奖励
message pcr {
    required int32 exp = 1; //经验
    required int32 soulpower = 2; //真气
    required int32 copper = 3; //铜钱
    required int32 feat = 4; //功勋
    required int32 prestige = 5; //声望
}

//挑战胜利
message personal_challenge_success_s2c {
    required uint16 msgid = 1;
    required int32 success_grade = 2; //胜利档次：1,完胜; 2,小胜; 3,险胜
    repeated i items = 3; //奖励的物品
    required pcr rewards = 4; //其他奖励
    required int32 is_first_blood = 5; //是否为首胜 1: 首胜, 0: 不是首胜
}

//个人挑战失败后给的提升方案
message pcs {
    required int32 type = 1; //方案类型
    required int32 expect_piece = 2; //期望件数 建议
    required int32 actual_piece = 3; //实际件数 当前
    required int32 expect_value = 4; //期望值 建议
    required int32 actual_value = 5; //实际值 当前
    required int32 fightingforce_diff = 6; //战斗力差值
}

//挑战失败
message personal_challenge_fail_s2c {
    required uint16 msgid = 1;
    required int32 fail_grade = 2; //失败档次：1,完败; 2,小败; 3,惜败
    required int32 fighting_force = 3; //玩家当前的战斗力
    repeated pcs schemes = 4; //失败时给出的战斗力提升方案
    required int32 need_fighting_force = 5; //挑战所需战斗力(推荐战斗力)
}

//领奖
message personal_challenge_reward_c2s {
    required uint16 msgid = 1;
}

//领奖返回
message personal_challenge_reward_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2; //领奖返回: 0,成功
}

//发起挑战返回
message personal_challenge_start_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2; //0: 挑战开始，进入副本
}
