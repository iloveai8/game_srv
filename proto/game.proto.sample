    required uint16 msgid = 1;
    required int32 type = 2; //类型:1好友,2仇人,3黑名单
    required string name = 3;
}

//添加玩家
message add_friend_s2c {
    required uint16 msgid = 1;
    required f new_relation = 2;
}


//更新玩家指定类型的值
message update_friend_value_s2c {
    required uint16 msgid = 1;
    required int64 roleid = 2; //玩家id
    required int32 type = 3; //类型
    required int32 value = 4; //更新值
}

//更新关系信息
message update_friend_s2c {
    required uint16 msgid = 1;
    required int64 roleid = 2; //玩家id
    repeated k key_value = 3; //更新的key_value值
}

//删除
message delete_friend_c2s {
    required uint16 msgid = 1;
    required int64 roleid = 2; //玩家id
    required int32 type = 3; //类型:1好友,2仇人,3黑名单
