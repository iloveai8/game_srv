message ak {
    required int32 key = 1;
    repeated int32 values = 2;
}

message init_auto_game_s2c {
    required uint16 msgid = 1;
    repeated ak aks = 2;
}

message auto_game_option_c2s {
    required uint16 msgid = 1;
    repeated ak aks = 2;
}

message auto_game_start_c2s {
