}

message set_sign_mind_c2s {
    required uint16 msgid = 1;
    repeated int32 minds = 2;
}


//关系结构
message f {
    required int64 roleid = 1;
    required string othername = 2; //对方名
    required int32 type = 3; //类型:1好友,2仇人,3黑名单,4:最近
    required int32 value = 4; //友好度(好友)/仇恨值(仇人)/交互时间(最近) 其他:无意义
    required int32 online = 5; //0不在线，1在线
    required int32 stall = 6; //0:无摆摊 1:有摆摊
    required string sign = 7; //签名
    repeated int32 curmind = 8; //心情列表
}

//初始化关系,分开发送.每个好友一条消息
message init_friend_s2c {
    required uint16 msgid = 1;
    required f relation = 2;
}

//有人加你为好友,弹框提示是否加对方为好友
message make_friend_with_you_s2c {
    required uint16 msgid = 1;
    required string name = 2;
}

//你的好友被击杀
message friend_bekilled_s2c {
    required uint16 msgid = 1;
    required string name = 2;
    required string mapname = 3;
    required string killername = 4;
}

//添加玩家
