    required int32 secretgoodsid = 1; //神秘商品ID
    required int32 remain_num = 2; //剩余物品数量
}


//神秘商店商品列表信息
message npc_secret_shop_goods_info_s2c {
    required uint16 msgid = 1;
    required int32 remaintimes = 2; //刷新剩余次数//神秘商店刷新次数
    required int32 totaltimes = 3; //刷新总次数
    repeated ssg secret_shop_goods = 4;
}

//购买神秘商品
message npc_secret_shop_buy_goods_c2s {
    required uint16 msgid = 1;
    required int64 npcid = 2;
    required int32 secretgoodsid = 3; //神秘商品ID
    required int32 goodsnum = 4; //物品数量
}

//购买神秘商品返回
message npc_secret_shop_buy_goods_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2;
}


//刷新神秘商店物品列表
message npc_secret_shop_refresh_goods_info_c2s {
    required uint16 msgid = 1;
    required int64 npcid = 2;
}


//刷新神秘商店物品列表返回
message npc_secret_shop_refresh_goods_info_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2;
}


/////////////////////////////////////////////////////////////////////////////////////
//			副本委托 2181 - 2190  intstance_consignation
/////////////////////////////////////////////////////////////////////////////////////
//副本委托请求
message instance_consign_req_c2s {
    required uint16 msgid = 1;
    required int32 instanceid = 2;
    required int32 consign_type = 3;
    required int32 times = 4;
}

//副本委托请求失败
message instance_consign_req_errror_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2;
}

//副本委托成功返回
message instance_consign_result_s2c {
    required uint16 msgid = 1;
}

//查看委托仓库
message consign_warehouse_list_c2s {
    required uint16 msgid = 1;
}

//返回委托仓库内容
message consign_warehouse_list_s2c {
    required uint16 msgid = 1;
    repeated i items = 2;
}

//从仓库中取物品
message consign_warehouse_fetch_c2s {
    required uint16 msgid = 1;
    required int32 index = 2; //index为0时代表全部取出
}

//仓库操作返回错误码
message consign_warehouse_error_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2;
}

//从仓库中删除物品
message consign_warehouse_destroy_c2s {
    required uint16 msgid = 1;
    required int32 index = 2; //index为0时代表全部删除
}

//初始化节日抽奖界面
message holiday_draw_init_s2c {
    required uint16 msgid = 1;
    repeated i items = 2; //物品列表,如果只有一个,则是待领取的奖品,如果有多个,则是尚未摇奖
    required int32 freenum = 3; //剩余免费次数
}

//开始抽奖
message holiday_draw_random_c2s {
    required uint16 msgid = 1;
    required int64 npcid = 2; //npcid
}

//返回抽奖结果
message holiday_draw_random_s2c {
    required uint16 msgid = 1;
    required int32 slot = 2; //所在抽奖槽位:槽位定义:顺时针循环1-16
    required i item = 3; //物品
}

//积分不足，失败
message holiday_draw_random_fail_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2; //抽奖失败消息
}

//领取物品
message holiday_draw_obt_c2s {
    required uint16 msgid = 1;
    required int64 npcid = 2; //npcid
}

//领取物品结果
message holiday_draw_obt_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2; //为0时代表领取成功
    required int32 freenum = 3; //剩余免费次数
}


////////////////////////////////////////////////////////////////////////////////////
//								主线闯关副本 2501-2510
///////////////////////////////////////////////////////////////////////////////////
//条件：任务状态quest_status
message qs {
    required int32 questid = 1;
    required int32 status = 2; //任务完成状态
}

//委托副本consignation
message coi {
    required int32 remaintime = 1; //剩余时间
}


//节信息section_info
message sei {
    required int32 sectionid = 1;
    required int32 iscomplete = 2; //已经完成首次通关,1:完成首次通关,下一个字段为评价等级;0:没有完成,下一个字段为完成度
    required int32 ratingorfinish = 3; //评价等级(星级数量)或者完成度(百分比%)
    repeated qs quest = 4; //条件：任务信息

}

//章信息chapter_info
message ci {
    required int32 chapterid = 1; //
    required int32 receivereward = 2; //领取通关奖励:1：领取;0:未领
    repeated sei section = 3; //节信息
}

//请求主线闯关副本详细信息
message tollgate_info_c2s {
    required uint16 msgid = 1;
}

//返回请求主线闯关副本详细信息
message tollgate_info_s2c {
    required uint16 msgid = 1;
    required int32 times = 2; //剩余进入次数
    required coi consigninfo = 3; //委托副本consignation
    repeated ci chapter = 4;
}

//请求挑战
message tollgate_start_c2s {
    required uint16 msgid = 1;
    required int32 chapterid = 2; //章id
    required int32 sectionid = 3; //节id
}

//请求挑战
message tollgate_start_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2;
}

// 奖励信息
message ri {
    required int32 exp = 1; //经验
    required int32 money = 2; //铜钱
    repeated i rewarditems = 3; //奖励物品
}

//关卡结束返回
message tollgate_end_s2c {
    required uint16 msgid = 1;
    required int32 istupo = 2; //是否突破，1为突破
    required int32 iscomplete = 3; //已经完成首次通关,1:完成首次通关,下一个字段为评价等级;0:没有完成,下一个字段为完成度
    required int32 ratingorfinish = 4; //评价等级(星级数量)或者完成度(百分比%)
    required ri rewardinfo = 5; //奖励信息
}

//清除扫荡CD
message tollgate_clear_cooldown_c2s {
    required uint16 msgid = 1;
}

message tollgate_clear_cooldown_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2; //为0时代表成功
}

//刷新扫荡CD
message tollgate_refresh_cooldown_s2c {
    required uint16 msgid = 1;
    required int32 times = 2; //剩余进入次数
    required coi consigninfo = 3; //委托副本consignation
}


//领取奖励
message tollgate_reward_c2s {
    required uint16 msgid = 1;
}

message tollgate_reward_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2; //为0时代表成功
}

//完美通关奖励
message tollgate_perfect_reward_c2s {
    required uint16 msgid = 1;
    required int32 chapterid = 2; //章id
}

message tollgate_perfect_reward_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2; //为0时代表成功
}

message random_box_s2c {
    required uint16 msgid = 1;
    required int32 boxpos = 2; //1:下  2:左下   3:左   4:左上   5:上   6:右上   7:右   8:右下
}

message effect {
    required int32 effectid = 1; //效果id
    repeated kvobject attrs = 2; //效果影响属性
}

message attack_target {
    required int64 targetid = 1; //目标id
    repeated effect effectlist = 2; //影响效果列表
}

message ios_attack_c2s {
    required uint16 msgid = 1;
    required int64 casterid = 2; //攻击者id
    required int32 skillid = 3; //技能id
    repeated attack_target targetlist = 4; //目标列表
    repeated kvobject casterattrs = 5; //攻击者影响属性
}
