//int型keyvalue结构
message ik {
    required int32 key = 1;
    required int32 value = 2;
}

//小地图信息请求
message map_detail_c2s {
    required uint16 msgid = 1;
}


//PVP据点信息
message pvp {
    required int32 occupy = 1; //占领方阵营
    required int32 attack = 2; //进攻方阵营
    required int32 state = 3; //1:占领中 2:占领后
}

//帮旗信息
message gf2 {
    required int64 guildid = 1;
    required string guildname = 2;
    required int32 hppersent = 3; //帮旗生命,百分比
}

//小地图信息返回
message map_detail_s2c {
    required uint16 msgid = 1;
    required int32 mapid = 2;
    required pvp pvp_info = 3; //PVP据点信息
    required gf2 guildflag = 4; //帮旗
}


//通知前台小地图面板变更
message map_change_s2c {
    required uint16 msgid = 1;
}


//领取Vip牛逼buff
message vip_call_buff_c2s {
    required uint16 msgid = 1;
}

///////////////////////////////////////////////////////////////////////////////////
//						  世界等级   2541-2560
///////////////////////////////////////////////////////////////////////////////////
//领取新手保护buff
message get_new_protect_c2s {
    required uint16 msgid = 1;
}

//返回世界等级
message world_level_s2c {
    required uint16 msgid = 1;
    required int32 world_level = 2;
}

//错误信息
message world_level_error_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2;
}

///////////////////////////////////////////////////////////////////////////////////
//						  名剑录
///////////////////////////////////////////////////////////////////////////////////
//请求名剑信息返回
message famous_sword_s2c {
    required uint16 msgid = 1;
    required int32 cur_level = 2;
    repeated int32 finish_list = 3;
}

//名剑操作
message summon_famous_sword_c2s {
    required uint16 msgid = 1;
    required int32 type = 2; //1:出战;2:召回
}

//名剑错误信息
message famous_sword_failed_s2c {
    required uint16 msgid = 1;
    required int32 errno = 2;
}



///////////////////////////////////////////////////////////////////////////////////
//						  剧情
///////////////////////////////////////////////////////////////////////////////////

message play_scenario_s2c {
    required uint16 msgid = 1;
    required int32 id = 2; //播放剧情ID
}

message play_scenario_finish_c2s {
    required uint16 msgid = 1;
    required int32 id = 2; //播放剧情ID
    required int32 is_jump = 3; //是否是跳过剧情:0:正常走完 1:跳过本剧情
}

